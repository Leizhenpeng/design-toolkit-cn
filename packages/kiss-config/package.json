{
    "name": "unplugin-kiss-config",
    "description": "🍙 自动生成figma,masterGo,jsDesign多个平台manifest.json的打包插件",
    "version": "0.1.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Leizhenpeng/design-tooltik-cn.git",
        "directory": "packages/kiss-config"
    },
    "bugs": {
        "url": "https://github.com/Leizhenpeng/design-tooltik-cn/issues"
    },
    "homepage": "https://github.com/Leizhenpeng/design-tooltik-cn/tree/main/packages/kiss-core#readme",
    "keywords": [
        "unplugin",
        "esbuild",
        "rollup",
        "vite",
        "webpack"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./types": {
            "require": "./dist/types.js",
            "import": "./dist/types.mjs"
        },
        "./*": "./*",
        "./esbuild": {
            "require": "./dist/esbuild.js",
            "import": "./dist/esbuild.mjs"
        },
        "./rollup": {
            "require": "./dist/rollup.js",
            "import": "./dist/rollup.mjs"
        },
        "./vite": {
            "require": "./dist/vite.js",
            "import": "./dist/vite.mjs"
        },
        "./webpack": {
            "require": "./dist/webpack.js",
            "import": "./dist/webpack.mjs"
        }
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch src"
    },
    "tsup": {
        "entryPoints": [
            "src/*.ts"
        ],
        "clean": true,
        "format": [
            "cjs",
            "esm"
        ],
        "dts": true
    },
    "dependencies": {
        "fs-extra": "^11.1.1",
        "kolorist": "^1.7.0",
        "unplugin": "^1.3.1",
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "tsup": "^6.7.0",
        "typescript": "^5.0.0"
    }
}