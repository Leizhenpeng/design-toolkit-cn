{
    "name": "kiss",
    "version": "1.0.0",
    "description": "同时兼容mastergo、jsDesign和figma的插件开发框架",
    "main": "index.js",
    "author": "RiverRay",
    "license": "MIT",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Leizhenpeng/design-tooltik-cn.git",
        "directory": "packages/kiss"
    },
    "bugs": {
        "url": "https://github.com/Leizhenpeng/design-tooltik-cn/issues"
    },
    "homepage": "https://github.com/Leizhenpeng/design-tooltik-cn/tree/main/packages/kiss#readme",
    "scripts": {
        "dev": "tsup-node --watch src",
        "build": "tsup-node --dts",
        "test": "vitest",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
        "test:coverage": "vitest --coverage",
        "patch:type": "esno script/patchType.ts",
        "clear:patch": "rimraf src/types/client/*.d.ts",
        "preinstall": "npx only-allow pnpm"
    },
    "keywords": [
        "figma",
        "mastergo",
        "jsdesign",
        "tooltik"
    ],
    "devDependencies": {
        "@figma/plugin-typings": "^1.57.1",
        "@jsdesigndeveloper/plugin-typings": "^1.0.9",
        "@mastergo/plugin-typings": "^1.3.1",
        "@types/node": "*",
        "@types/rimraf": "^3.0.2",
        "esno": "0.10.0",
        "figmaio": "^0.2.10",
        "fs-extra": "^10.0.0",
        "kolorist": "^1.6.0",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "tsup": "^6.5.0",
        "@jsdevtools/version-bump-prompt": "^6.1.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "dist"
    ]
}
